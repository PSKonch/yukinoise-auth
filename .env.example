# Переменные окружения для YukiNoise Auth

# Keycloak сервер
KEYCLOAK_SERVER_URL=http://localhost:8080

# Realm
KEYCLOAK_REALM=myrealm

# Client настройки
KEYCLOAK_CLIENT_ID=myapp
KEYCLOAK_CLIENT_SECRET=your-secret-here

# Опциональные настройки

# Проверка подписи токена (по умолчанию: true)
# KEYCLOAK_VERIFY_SIGNATURE=true

# Проверка срока действия токена (по умолчанию: true)
# KEYCLOAK_VERIFY_EXP=true

# Проверка audience (по умолчанию: true)
# KEYCLOAK_VERIFY_AUD=true

# Ожидаемый audience (по умолчанию: client_id)
# KEYCLOAK_AUDIENCE=myapp

# TTL кэша JWKS в секундах (по умолчанию: 3600)
# KEYCLOAK_JWKS_CACHE_TTL=3600

# Разрешенные алгоритмы (по умолчанию: ["RS256"])
# KEYCLOAK_ALGORITHMS=["RS256"]
